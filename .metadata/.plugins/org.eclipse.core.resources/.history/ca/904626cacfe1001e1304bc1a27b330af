package edu.kh.emp.common;

import java.sql.Connection;

public class JDBCTemplate {
 
	 private static Connection conn = null;
	
	 public static Connection getConnection(Connection conn) {
		 
		 
		 if(conn != null & !conn.isClosed()) {
			 
			 conn.close();
			 
		 }
		 
		 
		 
		 
		 
		 
		 return conn;
	 }
	 
	  
	 
}
